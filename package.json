{
  "name": "contact",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --offline --max-workers 2",
    "start:online": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --max-workers 2",
    "start:tunnel": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --tunnel --max-workers 2",
    "start:mem": "set NODE_OPTIONS=--max-old-space-size=4096 && expo start --offline --max-workers 2",
    "start:clean": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --offline --clear --max-workers 2",
    "android": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --android --offline --max-workers 2",
    "ios": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --ios --offline --max-workers 2",
    "web": "set NODE_OPTIONS=--max-old-space-size=8192 && expo start --web --offline --max-workers 2",
    "build:web": "set NODE_OPTIONS=--max-old-space-size=8192 && expo export --platform web && powershell -ExecutionPolicy Bypass -File inject-fonts.ps1",
    "deploy:web": "set NODE_OPTIONS=--max-old-space-size=8192 && expo export --platform web && powershell -ExecutionPolicy Bypass -File inject-fonts.ps1 && echo 'Build complete! Upload the dist folder to production.'"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.4.1",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/cli-platform-android": "^13.6.9",
    "@react-native-community/cli-platform-ios": "^13.6.9",
    "@react-native-community/cli-server-api": "^13.6.9",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@shopify/flash-list": "1.6.4",
    "@supabase/supabase-js": "^2.39.0",
    "@types/xlsx": "^0.0.35",
    "expo": "~51.0.0",
    "expo-constants": "~16.0.2",
    "expo-contacts": "~13.0.0",
    "expo-crypto": "~13.0.1",
    "expo-document-picker": "~12.0.1",
    "expo-file-system": "~17.0.0",
    "expo-font": "~12.0.9",
    "expo-haptics": "^15.0.8",
    "expo-image-manipulator": "~12.0.1",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.0",
    "expo-network": "~6.0.0",
    "expo-router": "~3.5.0",
    "expo-sharing": "~12.0.0",
    "expo-sqlite": "~14.0.0",
    "expo-status-bar": "~1.12.1",
    "libphonenumber-js": "^1.11.0",
    "lottie-react-native": "6.7.0",
    "papaparse": "^5.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.10",
    "react-query": "^3.39.3",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.0",
    "metro": "^0.80.12",
    "metro-transform-worker": "^0.80.12",
    "typescript": "~5.3.3"
  },
  "private": true
}
